{
  "name": "@neoskop/privacy-guard.js",
  "version": "0.0.3",
  "description": "",
  "keywords": [],
  "main": "dist/privacy-guard.umd.js",
  "module": "dist/privacy-guard.es5.js",
  "typings": "dist/types/privacy-guard.d.ts",
  "files": [
    "dist"
  ],
  "author": "Philipp Siekmann <siekmann@neoskop.de>",
  "repository": {
    "type": "git",
    "url": "https://github.com/neoskop/privacy-guard.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint --project tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "demo": "webpack-dev-server --progress --config webpack.config.js --host 0.0.0.0 --port 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "colors": "^1.1.2",
    "cross-env": "^5.0.1",
    "css-loader": "0.28.11",
    "cz-conventional-changelog": "^2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "^22.0.2",
    "lint-staged": "^7.0.0",
    "lodash.camelcase": "^4.3.0",
    "null-loader": "0.1.1",
    "postcss-cssnext": "3.1.0",
    "postcss-nested": "3.0.0",
    "postcss-sorting": "3.1.0",
    "prettier": "^1.4.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.57.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "1.6.1",
    "rollup-plugin-serve": "0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "style-loader": "0.21.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "4.3.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "core-js": "2.5.6",
    "template-mb": "2.0.6"
  }
}
